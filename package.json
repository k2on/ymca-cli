{
    "name": "ymca-cli",
    "version": "1.0.0",
    "description": "Manage YMCA Reservation from the command line",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "https://github.com/k2on/ymca-cli.git",
    "author": "Max Koon <maxk@nix2.io>",
    "license": "MIT",
    "private": false,
    "prettier": "@nix2/prettier-config",
    "scripts": {
        "start": "ts-node --transpile-only src/index.ts",
        "format": "prettier --write ./src/**",
        "test-format": "prettier --check ./src/**",
        "test": "mocha -r ts-node/register tests/**",
        "lint": "eslint src/**",
        "mk-docs": "typedoc --name \"Documentation\" && touch ./docs/.nojekyll",
        "prepublish": "tsc"
    },
    "devDependencies": {
        "@nix2/prettier-config": "^1.0.2",
        "@types/inquirer": "^7.3.1",
        "@types/mocha": "8.0.4",
        "@types/node": "14.14.21",
        "@typescript-eslint/eslint-plugin": "4.8.1",
        "@typescript-eslint/parser": "4.8.1",
        "eslint": "7.14.0",
        "eslint-plugin-jsdoc": "30.7.8",
        "mocha": "8.2.1",
        "prettier": "2.1.2",
        "ts-node": "9.0.0",
        "typedoc": "0.19.2",
        "typescript": "4.0.5"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "ak-scheduler": "^1.0.0",
        "colors": "^1.4.0",
        "commander": "^6.2.0",
        "inquirer": "^7.3.3",
        "moment": "^2.29.1",
        "ora": "^5.1.0"
    },
    "bin": {
        "ymca": "./dist/index.js"
    }
}
